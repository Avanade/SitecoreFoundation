<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      <handlebarHelpers>
        <processor type="SF.Feature.Handlebars.RegisterFormatDateHelper, SF.Feature.Handlebars" />
        <processor type="SF.Feature.Handlebars.RegisterIfEqualHelper, SF.Feature.Handlebars" />
        <processor type="SF.Feature.Handlebars.RegisterJSONHelper, SF.Feature.Handlebars" />
        <processor type="SF.Feature.Handlebars.RegisterPlaceholderHelper, SF.Feature.Handlebars" />
        <processor type="SF.Feature.Handlebars.RegisterUserSettingsHelper, SF.Feature.Handlebars" />
      </handlebarHelpers>
    </pipelines>
    <services>
      <configurator type="SF.Feature.Handlebars.Repositories.RegisterDependencies, SF.Feature.Handlebars" />
    </services>
  </sitecore>
</configuration>
