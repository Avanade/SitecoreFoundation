<?xml version="1.0"?>

<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <domainManager>
            <domains>
                <domain id="airspace" type="Sitecore.Security.Domains.Domain, Sitecore.Kernel">
                    <param desc="name">$(id)</param>
                    <ensureAnonymousUser>false</ensureAnonymousUser>
                </domain>
            </domains>
        </domainManager>
        <sites>
            
            <site name="airspace" patch:after="site[@name='modules_website']" 
                  targetHostName="airspace9.$(rootHostName)" 
                  hostName="airspace9.$(rootHostName)" 
                  SF.SiteConfiguration="{3F38A4EE-B011-48B8-9955-90CC07B9FF4E}" 
                  SF.ConfigRoot="{3F38A4EE-B011-48B8-9955-90CC07B9FF4E}" 
                  database="web" 
                  virtualFolder="/" 
                  physicalFolder="/" 
                  rootPath="/sitecore/content/" 
                  startItem="Sample Sites/Sites/Airspace/Home" 
                  dictionaryPath="/sitecore/content/Sample Sites/Sites/Airspace/Site Configuration/Site Dictionary" 
                  dictionaryAutoCreate="false" 
                  domain="starterKit" 
                  allowDebug="true" 
                  cacheHtml="true" 
                  htmlCacheSize="50MB" 
                  registryCacheSize="0" 
                  viewStateCacheSize="0" 
                  xslCacheSize="25MB" 
                  filteredItemsCacheSize="10MB" 
                  enablePreview="true" 
                  enableWebEdit="true" 
                  enableDebugger="true" 
                  disableClientData="false" 
                  cacheRenderingParameters="true" 
                  renderingParametersCacheSize="10MB" 
                  enableItemLanguageFallback="true" />
              
        </sites>
        <cacheSizes>
            <sites>
                <airspace>
                    <html>50MB</html>
                    <registry>0</registry>
                    <viewState>0</viewState>
                    <xsl>25MB</xsl>
                </airspace>
            </sites>
        </cacheSizes>
    </sitecore>
</configuration>