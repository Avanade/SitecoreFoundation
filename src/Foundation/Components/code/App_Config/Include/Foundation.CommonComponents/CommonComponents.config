<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <services>
      <configurator type="SF.Foundation.Components.Repositories.RegisterDependencies, SF.Foundation.Components" />
    </services>
    <commands>
      <!-- Registration of the AddToLocation command -->
      <command name="webedit:addToDataFolder" type="SF.Foundation.Components.Commands.AddItemToDataFolderCommand, SF.Foundation.Components"/>
    </commands>
    <pipelines>
      <getItemReferences>
        <processor type="SF.Foundation.Components.Publishing.PublishDataSourceChildItems, SF.Foundation.Components"/>
      </getItemReferences>
    </pipelines>
  </sitecore>
</configuration>
